{
    "Instrument_Settings": {
        "Active_Instrument": {
            "type": "_Label_Group",
            "AES70": "OcaDeviceManager",
            "description": "Displays information about the currently connected instrument. All fields are read-only sensors.",
            "fields": {
                "brand": {
                    "label": "Brand",
                    "value": "Keysight Technologies"                    
                },
                "device_model": {
                    "label": "Device Model",
                    "value": "Keysight"
                },
                "device_series": {
                    "label": "Device Series",
                    "value": "E440xB"
                },
                "device_serial_number": {
                    "label": "Serial Number",
                    "value": "E440xB"
                },
                "device_firmware": {
                    "label": "Firmware",
                    "value": "E440xB"
                },
                "visa_resource": {
                    "label": "VISA Resource",
                    "value": "TCPIP0::192.168.1.100::inst0::INSTR"
                },
                "Time_connected": {
                    "label": "Time Connected",
                    "value": "12:44.01"
                }
            }
        },
        "Frequency_Settings": {
            "type": "OcaBlock",
            "AES70": "OcaFrequencyActuator",
            "description": "A block for managing all frequency-related settings.",
            "fields": {
                "center_freq_MHz": {
                    "label": "Center Frequency",
                    "type": "_sliderValue",
                    "handler": "set_center_frequency",
                    "value": "150",
                    "min": "1",
                    "max": "10",
                    "step": "0.005",
                    "units": "MHz"
                },
                "span_freq_MHz": {
                    "label": "Frequency Span",
                    "type": "_sliderValue",
                    "handler": "set_span_frequency",
                    "value": "100",
                    "min": "1",
                    "max": "10",
                    "step": "0.005",
                    "units": "MHz"
                },
                "start_freq_MHz": {
                    "label": "Start Frequency",
                    "type": "_sliderValue",
                    "handler": "set_start_frequency",
                    "value": "0",
                    "min": "1",
                    "max": "10",
                    "step": "0.005",
                    "units": "MHz"
                },
                "stop_freq_MHz": {
                    "label": "Stop Frequency",
                    "type": "_sliderValue",
                    "handler": "set_stop_frequency",
                    "value": "3000",
                    "min": "1",
                    "max": "10",
                    "step": "0.005",
                    "units": "MHz"
                }
            }
        },
        "Bandwidth_Settings": {
            "type": "OcaBlock",
            "AES70": "OcaBandwidthActuator",
            "description": "A block for managing all bandwidth and sweep time settings.",
            "fields": {
                "rbw_MHz": {
                    "label": "Resolution Bandwidth",
                    "type": "_sliderValue",
                    "handler": "set_rbw",
                    "value": "1",
                    "min": "1",
                    "max": "10",
                    "units": "MHz"
                },
                "Sweep_time_s": {
                    "label": "Sweep Time",
                    "type": "_sliderValue",
                    "handler": "set_sweep_time",
                    "value": "1",
                    "min": "1",
                    "max": "10",
                    "units": "s"
                },
                "vbw_MHz": {
                    "label": "Video Bandwidth",
                    "type": "_sliderValue",
                    "handler": "set_vbw",
                    "value": "1",
                    "min": "1",
                    "max": "10",
                    "units": "MHz"
                },
                "vbw_auto_on": {
                    "label": "VBW Auto On",
                    "type": "_GuiButtonToggle",
                    "AES70": "OcaBooleanActuator",
                    "handler": "toggle_vbw_auto",
                    "options": {
                        "ON": {
                            "label": "Auto ON",
                            "selected": true
                        },
                        "OFF": {
                            "label": "Auto OFF",
                            "selected": false
                        }
                    }
                },
                "initiate_continuous_on": {
                    "label": "Continuous Initiate",
                    "type": "_GuiButtonToggle",
                    "AES70": "OcaBooleanActuator",
                    "handler": "toggle_continuous_initiate",
                    "options": {
                        "ON": {
                            "label": "ON",
                            "selected": true
                        },
                        "OFF": {
                            "label": "OFF",
                            "selected": false
                        }
                    }
                }
            }
        },
        "Amplitude_Settings": {
            "type": "OcaBlock",
            "AES70": "OcaGain",
            "description": "A block for managing all amplitude-related settings.",
            "fields": {
                "ref_level_dBm": {
                    "label": "Reference Level",
                    "type": "_sliderValue",
                    "handler": "set_ref_level",
                    "value": "-30",
                    "units": "dBm",
                    "min":"-100",
                    "max":"0",
                    "step":"10"
                },
                "preamp_on": {
                    "label": "Preamp On",
                    "type": "_GuiButtonToggle",
                    "AES70": "OcaBooleanActuator",
                    "handler": "toggle_preamp",
                    "options": {
                        "ON": {
                            "label": "ON",
                            "selected": false
                        },
                        "OFF": {
                            "label": "OFF",
                            "selected": true
                        }
                    }
                },
                "power_attenuation_dB": {
                    "label": "Power Attenuation",
                    "type": "_sliderValue",
                    "handler": "set_power_attenuation",
                    "value": "10",
                    "units": "dBm",
                    "min":"-100",
                    "max":"0",
                    "step":"10"
                },
                "high_sensitivity_on": {
                    "label": "High Sensitivity On",
                    "type": "_GuiButtonToggle",
                    "AES70": "OcaBooleanActuator",
                    "handler": "toggle_high_sensitivity",
                    "options": {
                        "ON": {
                            "label": "ON",
                            "selected": false
                        },
                        "OFF": {
                            "label": "OFF",
                            "selected": true
                        }
                    }
                }
            }
        },
