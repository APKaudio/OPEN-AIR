{
  "All Marker On": {
    "type": "OcaBlock",
    "AES70": "OcaMarkerManager",
    "description": "A block for managing global marker settings.",
    "fields": {
      "All_Markers_On": {
        "label_active": "All Markers ON",
        "label_inactive": "All Markers ON",
        "type": "_GuiButtonToggle",
        "AES70": "OcaBooleanActuator",
        "handler": "turn_all_markers_on",
        "options": {
          "ON": {
            "label_active": "All Markers ON",
            "label_inactive": "All Markers ON",
            "selected": true
          },
          "OFF": {
            "label_active": "All Markers OFF",
            "label_inactive": "All Markers OFF",
            "selected": false
          }
        }
      }
    }
  },
  "Marker 1": {
    "type": "OcaBlock",
    "description": "Settings for Marker 1",
    "fields": {
      "Active": {
        "label_active": "Active",
        "label_inactive": "Active",
        "type": "_GuiCheckbox",
        "AES70": "OcaSwitch",
        "handler": "toggle Marker 1 Active",
        "value": false
      },
      "X_MHz": {
        "label_active": "X",
        "label_inactive": "X",
        "type": "_Value",
        "AES70": "OcaFrequencyActuator",
        "value": "111",
        "units": "MHz"
      },
      "Y_dBm": {
        "label_active": "Y",
        "label_inactive": "Y",
        "type": "_Label",
        "AES70": "OcaLevelSensor",
        "value": "-30",
        "units": "dBm"
      }
    }
  },
