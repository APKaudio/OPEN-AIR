{
  "PSU_Panel": {
    "type": "OcaBlock",
    "description": "Power Supply Unit Control Panel",
    "fields": {
      "Voltage_Control": {
        "type": "OcaBlock",
        "description": "Voltage Output",
        "fields": {
          "voltage_set": {
            "type": "_TextInput",
            "label_active": "Voltage (V)",
            "value_default": "5.0",
            "message_details": {
              "PSU_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "VOLT <value>"
              }
            }
          }
        }
      },
      "Current_Control": {
        "type": "OcaBlock",
        "description": "Current Limit",
        "fields": {
          "current_set": {
            "type": "_TextInput",
            "label_active": "Current (A)",
            "value_default": "0.1",
            "message_details": {
              "PSU_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "CURR <value>"
              }
            }
          }
        }
      },
      "Output_Control": {
        "type": "OcaBlock",
        "description": "Output State",
        "fields": {
          "output_enable": {
            "type": "_GuiButtonToggle",
            "label_active": "ON",
            "label_inactive": "OFF",
            "state_default": false,
            "message_details": {
              "PSU_MODEL": {
                "type": "_GuiActuator",
                "trigger": false,
                "command_value": "OUTP <state>"
              }
            }
          }
        }
      }
    }
  }
}