
  
  {
    "nab": {
      "type": "OcaBlock",
      "description": "Measurements & Readbacks",
      "fields": {
        "Standard_Measurements": {
          "type": "OcaBlock",
          "description": "Voltage and Time Measurements",
          "fields": {
            "Generic_Model": {
              "type": "_GuiActuator",
              "active": true,
              "trigger": false,
              "message": ":MEASure:VPP? <source>;:MEASure:VRMS? <source>;:MEASure:FREQuency? <source>;:MEASure:PDUTy? <source>",
              "layout": { "height": 30, "sticky": "" }
            },
            "scpi_inputs": {
              "type": "OcaBlock",
              "fields": {
                "source": { "value": "CHANnel1", "type": "_Value", "description": "CHAN1-4, MATH, D0-D15" }
              }
            },
            "scpi_outputs": {
              "type": "OcaBlock",
              "fields": {
                "Vpp": { "value": "Peak-Peak", "type": "_Value" },
                "Vrms": { "value": "RMS", "type": "_Value" },
                "Freq": { "value": "Frequency", "type": "_Value" },
                "Pos_Duty": { "value": "Positive Duty Cycle", "type": "_Value" }
              }
            }
          }
        },
        "Cursor_Readback": {
          "type": "OcaBlock",
          "description": "Manual Cursor Values",
          "fields": {
            "Generic_Model": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":CURSor:MANual:AXValue?;:CURSor:MANual:AYValue?;:CURSor:MANual:BXValue?;:CURSor:MANual:BYValue?;:CURSor:MANual:XDELta?;:CURSor:MANual:YDELta?"
            },
            "scpi_outputs": {
              "type": "OcaBlock",
              "fields": {
                "AX_Val": { "value": "Cursor A X", "type": "_Value" },
                "AY_Val": { "value": "Cursor A Y", "type": "_Value" },
                "BX_Val": { "value": "Cursor B X", "type": "_Value" },
                "BY_Val": { "value": "Cursor B Y", "type": "_Value" },
                "Delta_X": { "value": "Delta X", "type": "_Value" },
                "Delta_Y": { "value": "Delta Y", "type": "_Value" }
              }
            }
          }
        },
        "Digital_State_Read": {
          "type": "OcaBlock",
          "description": "Check Digital Channel Activity",
          "fields": {
            "Generic_Model": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":LA:ACTive?;:LA:DISPlay?"
            },
            "scpi_outputs": {
              "type": "OcaBlock",
              "fields": {
                "Active_Group": { "value": "Active Group", "type": "_Value" },
                "LA_Display_State": { "value": "Global LA State", "type": "_Value" }
              }
            }
          }
        }
      }
    },
    "rig": {
      "type": "OcaBlock",
      "description": "Complex Configuration",
      "fields": {
        "Trigger_Setup_Complex": {
          "type": "OcaBlock",
          "description": "Advanced Trigger Types",
          "fields": {
            "Set_Pulse_Trigger": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":TRIGger:MODE PULSe;:TRIGger:PULSe:SOURce <source>;:TRIGger:PULSe:WHEN <condition>;:TRIGger:PULSe:WIDTh <width>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "source": { "value": "CHANnel1", "type": "_Value" },
                  "condition": { "value": "PGReater", "type": "_Value", "description": "PGReater, PLESs, NGReater, NLESs" },
                  "width": { "value": "1.0e-3", "type": "_Value" }
                }
              }
            },
            "Set_Pattern_Trigger": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":TRIGger:MODE PATTern;:TRIGger:PATTern:PATTern <pattern_str>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "pattern_str": { "value": "H,L,X,X", "type": "_Value", "description": "Pattern string (e.g. H,L,X,R)" }
                }
              }
            }
          }
        },
        "MSO_Digital_Setup": {
          "type": "OcaBlock",
          "description": "Logic Analyzer Config",
          "fields": {
            "Configure_Pod_Threshold": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":LA:POD<n>:THReshold <level_type>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "n": { "value": "1", "type": "_Value", "description": "Pod 1 (D0-D7) or 2 (D8-D15)" },
                  "level_type": { "value": "TTL", "type": "_Value", "description": "TTL, CMOS, ECL, or User Value" }
                }
              }
            },
            "Digital_Channel_Size": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":LA:SIZE <size>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "size": { "value": "SMAll", "type": "_Value", "description": "SMAll, MEDium, LARge" }
                }
              }
            }
          }
        },
        "Protocol_Decoder": {
          "type": "OcaBlock",
          "description": "Serial Bus Decoding",
          "fields": {
            "Setup_Decoder_Mode": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":DECoder<n>:MODE <mode>;:DECoder<n>:DISPlay ON;:DECoder<n>:FORMat <format>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "n": { "value": "1", "type": "_Value" },
                  "mode": { "value": "UART", "type": "_Value", "description": "PARallel, UART, SPI, IIC" },
                  "format": { "value": "ASCii", "type": "_Value", "description": "HEX, ASCii, DEC, BIN" }
                }
              }
            },
            "Setup_UART_Source": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":DECoder<n>:UART:TX <tx_src>;:DECoder<n>:UART:RX <rx_src>;:DECoder<n>:UART:BAUD <baud>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "n": { "value": "1", "type": "_Value" },
                  "tx_src": { "value": "CHANnel1", "type": "_Value" },
                  "rx_src": { "value": "CHANnel2", "type": "_Value" },
                  "baud": { "value": "9600", "type": "_Value" }
                }
              }
            }
          }
        }
      }
    },
    "set": {
      "type": "OcaBlock",
      "description": "Parameter Settings",
      "fields": {
        "Horizontal_System": {
          "type": "OcaBlock",
          "fields": {
            "Set_Timebase_Mode": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":TIMebase:MODE <mode>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "mode": { "value": "MAIN", "type": "_Value", "description": "MAIN, XY, ROLL" }
                }
              }
            },
            "Set_Main_Scale": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":TIMebase:MAIN:SCALe <scale>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "scale": { "value": "0.001", "type": "_Value" }
                }
              }
            }
          }
        },
        "Vertical_System_Analog": {
          "type": "OcaBlock",
          "fields": {
            "Set_Analog_Scale": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":CHANnel<n>:SCALe <volts>;:CHANnel<n>:OFFSet <offset>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "n": { "value": "1", "type": "_Value" },
                  "volts": { "value": "1.0", "type": "_Value" },
                  "offset": { "value": "0.0", "type": "_Value" }
                }
              }
            },
            "Set_Probe_Ratio": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":CHANnel<n>:PROBe <atten>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "n": { "value": "1", "type": "_Value" },
                  "atten": { "value": "10", "type": "_Value" }
                }
              }
            }
          }
        },
        "Vertical_System_Digital": {
          "type": "OcaBlock",
          "fields": {
            "Set_Digital_Position": {
              "type": "_GuiActuator",
              "active": true,
              "message": ":LA:DIGital<n>:POSition <pos>",
              "scpi_inputs": {
                "type": "OcaBlock",
                "fields": {
                  "n": { "value": "0", "type": "_Value", "description": "Digital Channel 0-15" },
                  "pos": { "value": "0", "type": "_Value", "description": "Position Index" }
                }
              }
            }
          }
        }
      }
    },
    "Do": {
      "type": "OcaBlock",
      "description": "Actions and Toggles",
      "fields": {
        "Run_Control": {
          "type": "OcaBlock",
          "fields": {
            "Run": {
              "type": "_GuiActuator",
              "label_inactive": "RUN",
              "Generic_Model": { "message": ":RUN" }
            },
            "Stop": {
              "type": "_GuiActuator",
              "label_inactive": "STOP",
              "Generic_Model": { "message": ":STOP" }
            },
            "Single": {
              "type": "_GuiActuator",
              "label_inactive": "SINGLE",
              "Generic_Model": { "message": ":SINGle" }
            },
            "Force_Trigger": {
              "type": "_GuiActuator",
              "label_inactive": "FORCE",
              "Generic_Model": { "message": ":TFORce" }
            },
            "Autoscale": {
              "type": "_GuiActuator",
              "label_inactive": "AUTO",
              "Generic_Model": { "message": ":AUToscale" }
            }
          }
        },
        "Digital_Display_Toggles": {
          "type": "OcaBlock",
          "fields": {
            "Turn_On_D0_Group": {
              "type": "_GuiActuator",
              "label_inactive": "D0-D7 ON",
              "Generic_Model": { "message": ":LA:DISPlay ON;:LA:POD1:DISPlay ON" }
            },
            "Turn_On_D8_Group": {
              "type": "_GuiActuator",
              "label_inactive": "D8-D15 ON",
              "Generic_Model": { "message": ":LA:DISPlay ON;:LA:POD2:DISPlay ON" }
            },
            "Turn_Off_All_Digital": {
              "type": "_GuiActuator",
              "label_inactive": "LA OFF",
              "Generic_Model": { "message": ":LA:DISPlay OFF" }
            }
          }
        },
        "Math_Actions": {
          "type": "OcaBlock",
          "fields": {
            "FFT_On": {
              "type": "_GuiActuator",
              "label_inactive": "FFT ON",
              "Generic_Model": { "message": ":MATH:DISPlay ON;:MATH:OPERator FFT" }
            },
            "Math_Off": {
              "type": "_GuiActuator",
              "label_inactive": "Math OFF",
              "Generic_Model": { "message": ":MATH:DISPlay OFF" }
            }
          }
        }
      }
    }
  }
