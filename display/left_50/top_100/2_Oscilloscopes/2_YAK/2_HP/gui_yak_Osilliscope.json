{
  "nab": {
    "type": "OcaBlock",
    "description": "Oscilloscope Measurements & Readbacks",
    "fields": {
      "Measure_Voltage": {
        "type": "OcaBlock",
        "description": "Standard Voltage Measurements",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "trigger": false,
            "message": ":MEASure:VPP? <source>;:MEASure:VRMS? <source>;:MEASure:VMAX? <source>;:MEASure:VMIN? <source>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "source": {
                "value": "CHANnel1",
                "type": "_Value",
                "description": "Source channel (e.g., CHANnel1, CHANnel2)."
              }
            }
          },
          "scpi_outputs": {
            "type": "OcaBlock",
            "description": "Measurement Results",
            "fields": {
              "Vpp_Volts": { "value": "Peak-to-peak voltage", "type": "_Value" },
              "Vrms_Volts": { "value": "RMS voltage", "type": "_Value" },
              "Vmax_Volts": { "value": "Maximum voltage", "type": "_Value" },
              "Vmin_Volts": { "value": "Minimum voltage", "type": "_Value" }
            }
          }
        }
      },
      "Measure_Time": {
        "type": "OcaBlock",
        "description": "Time & Frequency Measurements",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "trigger": false,
            "message": ":MEASure:FREQuency? <source>;:MEASure:PERiod? <source>;:MEASure:RISEtime? <source>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "source": { "value": "CHANnel1", "type": "_Value" }
            }
          },
          "scpi_outputs": {
            "type": "OcaBlock",
            "fields": {
              "Frequency_Hz": { "value": "Frequency in Hz", "type": "_Value" },
              "Period_s": { "value": "Period in seconds", "type": "_Value" },
              "Risetime_s": { "value": "Risetime in seconds", "type": "_Value" }
            }
          }
        }
      },
      "Get_Waveform_Data": {
        "type": "OcaBlock",
        "description": "Acquire Waveform Data Block",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "trigger": false,
            "message": ":WAVeform:PREamble?;:WAVeform:DATA?",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_outputs": {
            "type": "OcaBlock",
            "fields": {
              "Preamble": { "value": "Format, type, points, count, xinc, xorg, xref, yinc, yorg, yref", "type": "_Value" },
              "Waveform_Binary": { "value": "IEEE 488.2 Definite Length Block Data", "type": "_Value" }
            }
          }
        }
      }
    }
  },
  "rig": {
    "type": "OcaBlock",
    "description": "Oscilloscope Configuration",
    "fields": {
      "Trigger_Setup_Edge": {
        "type": "OcaBlock",
        "description": "Configure Edge Trigger",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "message": ":TRIGger:MODE EDGE;:TRIGger:EDGE:SOURce <source>;:TRIGger:EDGE:SLOPe <slope>;:TRIGger:EDGE:LEVel <level>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "source": { "value": "CHANnel1", "type": "_Value" },
              "slope": { "value": "POSitive", "type": "_Value", "description": "POSitive or NEGative" },
              "level": { "value": "0.5", "type": "_Value", "description": "Trigger level in Volts" }
            }
          }
        }
      },
      "Waveform_Setup": {
        "type": "OcaBlock",
        "description": "Configure Waveform Transfer",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "message": ":WAVeform:SOURce <source>;:WAVeform:FORMat <format>;:WAVeform:POINts <points>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "source": { "value": "CHANnel1", "type": "_Value" },
              "format": { "value": "BYTE", "type": "_Value", "description": "BYTE, WORD, or ASCII" },
              "points": { "value": "1000", "type": "_Value", "description": "100, 250, 500, 1000, 2000, MAXimum" }
            }
          }
        }
      }
    }
  },
  "set": {
    "type": "OcaBlock",
    "description": "Individual Parameter Sets",
    "fields": {
      "Set_Timebase": {
        "type": "OcaBlock",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "message": ":TIMebase:SCALe <scale_s>;:TIMebase:POSition <pos_s>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "scale_s": { "value": "0.001", "type": "_Value", "description": "Time per division in seconds" },
              "pos_s": { "value": "0.0", "type": "_Value", "description": "Time delay from trigger" }
            }
          }
        }
      },
      "Set_Channel_Vertical": {
        "type": "OcaBlock",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "message": ":CHANnel<n>:SCALe <volts_div>;:CHANnel<n>:OFFSet <offset_v>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "n": { "value": "1", "type": "_Value", "description": "Channel Number (1, 2, etc)" },
              "volts_div": { "value": "1.0", "type": "_Value" },
              "offset_v": { "value": "0.0", "type": "_Value" }
            }
          }
        }
      },
      "Set_Acquisition_Type": {
        "type": "OcaBlock",
        "fields": {
          "Generic_Model": {
            "type": "_GuiActuator",
            "active": true,
            "message": ":ACQuire:TYPE <type>",
            "layout": { "height": 30, "sticky": "" }
          },
          "scpi_inputs": {
            "type": "OcaBlock",
            "fields": {
              "type": { "value": "NORMal", "type": "_Value", "description": "NORMal, AVERage, or PEAK" }
            }
          }
        }
      }
    }
  },
  "Do": {
    "type": "OcaBlock",
    "description": "Automatic Actions and Toggles",
    "fields": {
      "Run_Control": {
        "type": "OcaBlock",
        "fields": {
          "Do_Run": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "RUN",
            "layout": { "height": 30, "sticky": "" },
            "Generic_Model": { "message": ":RUN" }
          },
          "Do_Stop": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "STOP",
            "layout": { "height": 30, "sticky": "" },
            "Generic_Model": { "message": ":STOP" }
          },
          "Do_Single": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "SINGLE",
            "layout": { "height": 30, "sticky": "" },
            "Generic_Model": { "message": ":SINGle" }
          },
          "Do_Autoscale": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "AUTOSCALE",
            "layout": { "height": 30, "sticky": "" },
            "Generic_Model": { "message": ":AUToscale" }
          }
        }
      },
      "Channel_States": {
        "type": "OcaBlock",
        "fields": {
          "Do_Ch1_Display_ON": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "CH1 ON",
            "Generic_Model": { "message": ":CHANnel1:DISPlay ON" }
          },
          "Do_Ch1_Display_OFF": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "CH1 OFF",
            "Generic_Model": { "message": ":CHANnel1:DISPlay OFF" }
          }
        }
      },
      "System_Actions": {
        "type": "OcaBlock",
        "fields": {
          "Do_Clear_Status": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "CLS",
            "Generic_Model": { "message": "*CLS" }
          },
          "Do_Reset": {
            "type": "_GuiActuator",
            "AES70": "OcaBooleanActuator",
            "active": true,
            "label_inactive": "RST",
            "Generic_Model": { "message": "*RST" }
          }
        }
      }
    }
  }
}