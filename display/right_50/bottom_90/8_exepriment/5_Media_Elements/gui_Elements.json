{
   "media_and_specialty": {
    "type": "OcaBlock",
    "description": "Media & Specialty Controls (3-Column Grid)",
    "layout_columns": 3,
    "fields": {
      "media_panel": {
        "type": "OcaBlock",
        "description": "Visuals",
        "fields": {
            "img": { "type": "_ImageDisplay", "label_active": "Image", "value_default": "assets/images/flux_capacitor.png", "layout": {"width": 150, "height": 100} },
            "anim_gif": { "type": "_AnimationDisplay", "label_active": "GIF Animation", "gif_path": "assets/images/time_circuits.gif", "layout": {"width": 150, "height": 100} },
            "anim_static": { "type": "_ImageDisplay", "label_active": "GIF Static Frame", "value_default": "assets/images/time_circuits.gif.png", "layout": {"width": 150, "height": 100} }
        }
      },
      "custom_controls": {
        "type": "OcaBlock",
        "description": "Advanced Metering & Controls",
        "fields": {
            "fader_standard": { "type": "_Fader", "label_active": "Standard Fader", "value_default": 50, "layout": {"height": 200, "width": 60} },
            "needle": { "type": "_NeedleVUMeter", "label_active": "Analog VU", "value_default": -5, "min": -20, "max": 3, "size": 200 },
            "vu_meter": { "type": "_VUMeter", "label_active": "Digital VU", "value_default": -10, "min": -40, "max": 0, "layout": {"width": 60, "height": 200} },
            "knob_example": { "type": "_Knob", "label_active": "Knob Control", "value_default": 0.5, "min": 0.0, "max": 1.0, "resolution": 0.01, "size": 100 },
            "knob_blue": { "type": "_Knob", "label_active": "Blue Knob", "indicator_color": "#0000FF", "value_default": 25, "min": 0, "max": 100, "size": 50 },
            "knob_green": { "type": "_Knob", "label_active": "Green Knob", "indicator_color": "#00FF00", "value_default": 50, "min": 0, "max": 100, "size": 50 },
            "knob_purple": { "type": "_Knob", "label_active": "Purple Knob", "indicator_color": "#800080", "value_default": 75, "min": 0, "max": 100, "size": 50 },
            "panner_example": { "type": "_Panner", "label_active": "Panner Control", "value_default": 0, "min": -100, "max": 100, "size": 100 },
            "custom_fader": { "type": "_CustomFader", "label_active": "Custom Fader", "value_default": 75, "layout": {"height": 200, "width": 60} },
            "custom_horizontal_fader": { "type": "_CustomHorizontalFader", "label_active": "Custom Horizontal Fader", "value_default": 50, "layout": {"width": 200, "height": 60} }
        }
